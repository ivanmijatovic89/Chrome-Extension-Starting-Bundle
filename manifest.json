{
  "name": "OpenSea Relist",
  "version": "1.2",
  "description": ":D",
  "background": {"page": "background.html"},
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "runtime_allowed_hosts":[ "http://*.opensea.io/*", "https://*.opensea.io/*"],
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "moment.min.js", "background.js" ],
    "css": ["customStyles.css"],
    "matches": [ "http://*.opensea.io/*", "https://*.opensea.io/*"]
  }]
}